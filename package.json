    "clipbrd": "^1.0.2",
    "date-fns": "^2.16.1",
    "entity-decode": "^2.0.2",
    "from-exponential": "^1.0.0",
    "lodash-es": "^4.17.15",
    "minter-js-sdk": "github:MinterTeam/minter-js-sdk#chilinet",
    "minterjs-tx": "^9.0.0-alpha.2",
    "minterjs-util": "^0.18.0-alpha.2",
    "nuxt": "^2.14.4",
