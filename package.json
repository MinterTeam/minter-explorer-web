{
  "name": "minter-explorer-web",
  "version": "1.0.0",
  "description": "Minter Explorer Website",
  "author": "shrpne <shrpne@gmail.com>",
  "private": true,
  "scripts": {
    "prod": "npm run production",
    "production": "npm run build",
    "dev": "gulp & nuxt --spa --port=3009",
    "build": "gulp once && nuxt build --spa",
    "start": "nuxt start --spa --port=3009",
    "generate": "gulp once && nuxt generate --fail-on-error",
    "analyze": "gulp once && nuxt build --spa --analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "precommit": "echo 'Pre-commit checks...' && npm run lint"
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "@material/snackbar": "^3.1.0",
    "@minterteam/footer-links": "^3.0.0",
    "axios": "^0.19.0",
    "big.js": "^5.2.2",
    "centrifuge": "^2.2.1",
    "chart.js": "^2.8.0",
    "clipbrd": "^1.0.2",
    "date-fns": "^2.0.1",
    "entity-decode": "^2.0.2",
    "from-exponential": "^1.0.0",
    "lodash-es": "^4.17.15",
    "minter-js-sdk": "^0.20.0",
    "minterjs-tx": "^6.1.0",
    "minterjs-util": "^0.12.0",
    "nuxt": "^2.9.2",
    "pretty-num": "^0.3.0",
    "qrcode.vue": "^1.6.2",
    "sockjs-client": "^1.4.0",
    "thousands": "^1.0.1",
    "vue-inline-svg": "^1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "beeper": "^2.0.0",
    "del": "^5.1.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-imagemin": "^6.1.0",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "imagemin-mozjpeg": "^8.0.0",
    "postcss-normalize": "^8.0.1",
    "pre-commit": "^1.2.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
